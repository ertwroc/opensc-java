#
# Default makefile variable settings.
#
ac CFLAGS=-O2 -Wall
ac CXXFLAGS=-O2 -Wall
test { `gcc --version` =~ /4\.[1-9]\.[0-9]/ } then {ac CFLAGS=-O2 -fno-stack-protector -Wall}
test { `gcc --version` =~ /4\.[1-9]\.[0-9]/ } then {ac CXXFLAGS=-O2 -fno-stack-protector -Wall}
